    "# Import QRCode from pyqrcode\n",
    "import pyqrcode\n",
    "import png\n",
    "from pyqrcode import QRCode\n",
    "\n",
    "\n",
    "# String which represents the QR code\n",
    "s = \"Rohit Bhandare\"\n",
    "\n",
    "# Generate QR code\n",
    "url = pyqrcode.create(s)\n",
    "\n",
    "url.svg(\"myQR.jpeg\", scale = 8)\n",
    "\n",
    "url.png('myQR.jpeg', scale = 6)\n",
    "print(\"QR created\")\n",
    "\n"
   ]
